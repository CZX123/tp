@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AppParser" as AppParser LOGIC_COLOR
participant ":ContactDeleteCommandParser" as ContactDeleteCommandParser LOGIC_COLOR
participant "d:ContactDeleteCommand" as ContactDeleteCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("contact delete 1")
activate LogicManager

LogicManager -> AppParser : parseCommand("contact delete 1")
activate AppParser

create ContactDeleteCommandParser
AppParser -> ContactDeleteCommandParser
activate ContactDeleteCommandParser

ContactDeleteCommandParser --> AppParser
deactivate ContactDeleteCommandParser

AppParser -> ContactDeleteCommandParser : parse("1")
activate ContactDeleteCommandParser

create ContactDeleteCommand
ContactDeleteCommandParser -> ContactDeleteCommand
activate ContactDeleteCommand

ContactDeleteCommand --> ContactDeleteCommandParser :
deactivate ContactDeleteCommand

ContactDeleteCommandParser --> AppParser : d
deactivate ContactDeleteCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
ContactDeleteCommandParser -[hidden]-> AppParser
destroy ContactDeleteCommandParser

AppParser --> LogicManager : d
deactivate AppParser

LogicManager -> ContactDeleteCommand : execute(m)
activate ContactDeleteCommand

ContactDeleteCommand -> Model : deleteContact(1)
activate Model

Model --> ContactDeleteCommand
deactivate Model

create CommandResult
ContactDeleteCommand -> CommandResult
activate CommandResult

CommandResult --> ContactDeleteCommand
deactivate CommandResult

ContactDeleteCommand --> LogicManager : r
deactivate ContactDeleteCommand

[<--LogicManager
deactivate LogicManager
@enduml
